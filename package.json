{
  "name": "react-socketio-demo",
  "description": "This project is to practice implementing sockets in a React application.",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "npx json-server ./dataServer/db.json",
    "dev:socket": "bun --watch ./socketServer",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:staged": "npx lint-staged",
    "preview": "vite preview",
    "test": "vitest",
    "test:once": "vitest --run",
    "prepare": "husky install"
  },
  "dependencies": {
    "express": "4.21.2",
    "json-server": "1.0.0-beta.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io": "4.8.1",
    "socket.io-client": "4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/express": "5.0.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "husky": "9.1.7",
    "jsdom": "26.0.0",
    "lint-staged": "^15.5.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vitest": "3.0.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
